{
  "Routes": [
    {
      "UpstreamPathTemplate": "/api/v0/users",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      
      "DownstreamScheme": "http",
      "DownstreamPathTemplate": "/api/v0/users",
      "DownstreamHostAndPorts": [{
        "Host": "user-service",
        "Port": 80
      }]
    },{
      "UpstreamPathTemplate": "/api/v0/users/{userId}",
      "UpstreamHttpMethod": [ "Put", "Delete"],

      "DownstreamScheme": "http",
      "DownstreamPathTemplate": "/api/v0/users/{userId}",
      "DownstreamHostAndPorts": [{
        "Host": "user-service",
        "Port": 80
      }]
    },

    {
      "UpstreamPathTemplate": "/api/v0/syllabus",
      "UpstreamHttpMethod": [ "Get", "Post" ],

      "DownstreamScheme": "http",
      "DownstreamPathTemplate": "/api/v0/syllabus",
      "DownstreamHostAndPorts": [{
        "Host": "syllabus-service",
        "Port": 80
      }]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:800"
  }
}